{
    "manifest_version" : 2,
    "name" : "Fake Filler",
    "version" : "1.0.0",
    "description" : "Fake Filler for input dummy data",
    "icons" : {
        "128" : "icon_128.png",
        "48" : "icon_48.png",
        "16" : "icon_16.png"
    },
    "permissions" : [ 
        "contextMenus",
        "tabs",
        "storage"
    ],
    "browser_action" : {
        "default_icon" : "icon_48.png"
    },
    "background" : {
        "scripts" : ["eventPage.js","jquery-3.5.1.min.js"],
        "persistent" : false
    },
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js" : ["content.js","jquery-3.5.1.min.js"]
        }
    ]

}